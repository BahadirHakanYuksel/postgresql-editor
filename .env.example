# Production modunda gerçek PostgreSQL bağlantısı için
# DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# API Rate Limiting
# MAX_REQUESTS_PER_MINUTE=100

# Cache ayarları
# CACHE_TTL=300

# Debug mode
# DEBUG=false
